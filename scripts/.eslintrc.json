{
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
            2
    ],
    "linebreak-style": 0
    ,
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "keyword-spacing": [
      "error", {
        "before": true,
        "after": true
      }
    ]
  }
}
