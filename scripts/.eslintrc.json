{
  "extends": "eslint:recommended",
    "globals": {
        "Game": true,
        "NetworkIds": true,
        "Queue": true,
        "io": true,
        "$": true
    },
  "rules": {
    "indent": [
      "error",
            2
    ],
    "linebreak-style": 0
    ,
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "keyword-spacing": [
      "error", {
        "before": true,
        "after": true
      }
    ]
  }
}
